<div class="hero-unit">
  <h2>All Commits</h2>
  <%- linkTo('Create a new Commit', addCommitPath, {class: 'btn pull-right'}); %>
</div>

<div id="commits-list">
<% if (commits) { %>
<% for (var i = 0, ii = commits.length; i < ii; i++) { %>
  <div class="row list-item" id="commit-<%= commits[i].id; %>">
    <div class="span8">
      <h3><%- linkTo(commits[i].id, commitPath(commits[i].id)); %></h3>
    </div>
    <div class="span4">
      <h3><i class="icon-list-alt"></i><%= commits[i].id; %></h3>
    </div>
  </div>
<% } %>
<% } %>
</div>